			font-weight bold
			font-size 12px
			color white
			padding 3px 6px
			letter-spacing 1px
			border-radius 3px
		&-inner
			// margin auto
			display flex
			align-items center
			justify-content center
			padding 20px
			position absolute
			top 0
			bottom 0
			left 0
			right 0
			&.blog
				flex-direction: column;
				.title
					text-shadow: 0 2px 4px rgba(black, .5);
			&.type2
				flex-direction: column;
				justify-content: center;
				.subtitle
					margin initial
		.title
			text-align center
			// font-size 48px
			font-size 30px
			font-family: 'Montserrat',sans-serif;
			font-weight bold
			color white
			max-width 650px
		.subtitle

/*------------------------------------*
	$ SLIDER
*------------------------------------*/
.slider
	position relative
	overflow: hidden;
	&.blog
		overflow: inherit;
	&-nav
		display flex
		justify-content center
		padding .5rem
		position absolute
		bottom 0
		left 50%
		transform translateX(-50%)
		&-item
			// font-size .6rem
			font-size .65rem
			color #ddd
			// color white
			// margin .25rem
			// padding .25rem

			margin .15rem
			padding .15rem

			cursor pointer
			&.active
				color #bbb
				color white
	&-items
		white-space nowrap
		font-size 0
		// overflow hidden
	&-item
		display inline-block
		vertical-align top
		width 100%
		white-space normal
		transition transform 700ms ease-out
		background-size cover
		background-position center center
		// z-index 2
		// min-height 400px
		min-height 300px
		// display flex
		position relative
		&-tag
			position absolute
			top 20px
			left 20px
			background #41a674

/*------------------------------------*
	$ ARTIST CARD
*------------------------------------*/
.artist-card
	border solid 1px #ddd
	position relative
	&-info
		display flex
		align-items center
		padding 10px
		>div
			flex 1
		img
			height 50px
			width 50px
			border-radius 50%
			margin-right 10px
		p
			font-family 'Montserrat', sans-serif
			font-size 10px
			color #666
			overflow ellipsis
			&:not(:last-child)
				padding-bottom 5px
			i
				font-size 1.1em
		.name
			font-weight bold
	&-works
		display flex
		flex-wrap wrap
	&-work
		width 50%
		height 110px
		background #ddd
		box-sizing: border-box;
		>div
			background-size cover
			background-position center center
			height 100%
			width 100%
		&.empty
			&:nth-child(1)
				border-right solid 1px white
				border-bottom solid 1px white
			&:nth-child(2)
				border-left solid 1px white
				border-bottom solid 1px white
			&:nth-child(3)
				border-right solid 1px white
				border-top solid 1px white
			&:nth-child(4)
				border-left solid 1px white
				border-top solid 1px white
	&-follow
		padding 12px
		button
			display table
			margin auto
	.invitation
		height 348px
		box-sizing: border-box;
		display flex
		background-size cover
		background-position center center
		padding 10px
		.inner
			margin auto
			text-align center
			p
				color white
				font-size 20px
				font-family 'Montserrat', sans-serif
				font-weight bold
				line-height 1.25
				text-shadow 0px 2px 4px rgba(black, .5)
			a
				letter-spacing 2px
				background #277cea
				color white
				font-size 10px
				font-family 'Montserrat', sans-serif
				font-weight bold
				padding 8px 40px
				margin-top 20px
				display inline-block
				border-radius 100px


/*------------------------------------*
	$ COLLECTION CARD
*------------------------------------*/
.collection-card
	border solid 1px #ddd
	position relative
	&-stats
		padding 12px
		p
			font-family 'Montserrat', sans-serif
			font-size 10px
			color #666
			&:first-child
				font-weight bold
				padding-bottom 5px
	&-images
		display flex
		flex-wrap wrap
	&-image
		background-size cover
		background-position center center
		height 110px
		background #ddd
		&:not(:first-child)
			width 50%
			&.empty
				box-sizing border-box
				background: #ddd;
				border-top: solid 1px white;
				box-sizing: border-box;
		&:first-child
			width 100%
			&.empty
				border-bottom solid 1px white
				box-sizing border-box
		&:last-child
			&.empty
				border-left: solid 1px white;
		&:nth-child(2)
			&.empty
				border-right: solid 1px white;

	&-artist
		padding 12px
		display flex
		align-items center
		> div
			flex 1
		img
			height 30px
			width 30px
			border-radius 50%
		p
			font-family 'Montserrat', sans-serif
			font-size 10px
			color #666
			&:last-child
				font-weight bold
				padding-top 5px



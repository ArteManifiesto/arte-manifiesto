/*------------------------------------*
	$ HOME GRID
*------------------------------------*/

.home-grid
	display flex
	justify-content center
	&-element
		margin 20px 10px
		display block
		position relative
		.cover
			position absolute
			height 100%
			width 100%
			&:hover
				background rgba(black, .15)
				transition background .35s
	&-column
		&.tablet
			display none
		&.desktop
			display none

@media (min-width: 600px)
	.home-grid
		&-column
			&.tablet
				display block

@media (min-width: 1000px)
	.home-grid
		&-column
			&.desktop
				display block

body
	overflow-x hidden

.flash-message
	// position absolute
	position fixed
	bottom 20px
	// left 0
	width 250px
	// border solid 1px #999
	box-shadow 0 2px 4px 0 rgba(black, .5)
	border-radius 0 4px 4px 0
	overflow hidden
	z-index 2
	background white
	transform translatex(-100%)
	&.error
		.top-bar
			background linear-gradient(left, #9f041b, #f5515f)
	&.succes
		.top-bar
			background linear-gradient(left, #3023ae, #c86dd7)
	.top-bar
		height 8px
	.content
		padding 20px 10px 10px 10px
		overflow hidden
		position relative
		.closed
			position: absolute;
			top: 10px;
			right: 10px;
			font-family: 'Montserrat',sans-serif;
			color #9b9b9b
			cursor pointer
		p
			font-family: 'Montserrat',sans-serif;
			font-size 12px
			color charcoal-grey
			line-height 1.5
		button
			font-family: 'Montserrat',sans-serif;
			font-size 9px
			color charcoal-grey
			letter-spacing 1.5px
			background white
			padding 6px 7px
			border solid 1px #c2c2c2
			float right
			margin-top 10px
			cursor pointer


.fadeIn
	animation 5s fadeIn

.fadeIn2
	// animation 5s fadeIn2
	transform translatex(0)
	transition transform .5s


